{
  "name": "asyncjs-static-site",
  "private": true,
  "version": "1.0.0",
  "description": "Async Static Site",
  "scripts": {
    "this-needs-to-go": "gulp",
    "clean": "rm -rf ./dist/",
    "copy:images": "sync-files -v ./_assets/images/ ./dist/img/",
    "copy:wp-files": "sync-files -v ./_assets/wp/ ./dist/wp/",
    "sitemap": "sitemap-static --prefix https://asyncjs.com --pretty ./dist/ > ./dist/sitemap.xml",
    "build": "npm-run-all copy:* this-needs-to-go",
    "test": "npm-run-all clean build",
    "browser": "browser-sync start -s ./dist/ -f ./dist/** --reload-debounce 3000",
    "watch": "watch-spawn npm run build -p '**' -p '!{.git,node_modules,dist}/**' --initial --skip-if-running",
    "start": "npm-run-all --parallel browser watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/asyncjs/asyncjs.github.com.git"
  },
  "author": "humans.txt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asyncjs/asyncjs.github.com/issues"
  },
  "homepage": "https://asyncjs.com/",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.18.8",
    "graceful-fs": "^4.1.11",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-front-matter": "^1.3.0",
    "gulp-htmlhint": "^0.3.1",
    "gulp-jscs": "^4.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-scss-lint": "^0.4.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^2.1.2",
    "gulp-uncss": "^1.0.6",
    "gulpsmith": "^0.5.5",
    "handlebars": "^4.0.6",
    "markdownlint": "^0.2.0",
    "marked": "^0.3.6",
    "metalsmith": "^2.3.0",
    "metalsmith-build-date": "^0.2.0",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-collections-archive": "^1.0.0",
    "metalsmith-collections-filter": "^1.0.1",
    "metalsmith-collections-limit": "^1.0.0",
    "metalsmith-collections-offset": "^1.0.0",
    "metalsmith-drafts": "0.0.1",
    "metalsmith-excerpts": "^1.1.0",
    "metalsmith-feed": "^0.2.0",
    "metalsmith-google-analytics": "0.0.1",
    "metalsmith-in-place": "^1.4.4",
    "metalsmith-layouts": "^1.7.0",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-metadata": "0.0.4",
    "metalsmith-metadata-in-filename": "^1.2.0",
    "metalsmith-more": "^0.2.0",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-redirect": "^2.1.0",
    "metalsmith-register-helpers": "^0.2.0",
    "metalsmith-tags": "^2.0.0",
    "moment": "^2.18.1",
    "npm-run-all": "^4.0.2",
    "sitemap-static": "^0.4.2",
    "striptags": "^2.2.1",
    "sync-files": "^1.0.3",
    "through2": "^2.0.3",
    "watch-spawn": "^2.0.0"
  }
}
